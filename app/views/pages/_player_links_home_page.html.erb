<div class="player-name-links">
  <%
    natalie_jens = @players.find{ |p| p.name == 'Natalie Jens' }
    jada_donaldson = @players.find{ |p| p.name == 'Jada Donaldson' }
    paige_banks = @players.find{ |p| p.name == 'Paige Banks' }
    paige_yagodinski = @players.find{ |p| p.name == 'Paige Yagodinski' }
    anna_lutz = @players.find{ |p| p.name == 'Anna Lutz' }
    kate_hosking = @players.find{ |p| p.name == 'Kate Hosking' }
    ramsey_mcgrew = @players.find{ |p| p.name == 'Ramsey McGrew' }
    laura_nickel = @players.find{ |p| p.name == 'Laura Nickel' }
