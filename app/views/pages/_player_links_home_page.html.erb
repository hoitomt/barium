  %>
  <div class="row no-gutters">
    <div class="col-md" align="center">
      <%= link_to "##{natalie_jens.number} #{natalie_jens.name.split(' ').last}", team_manager.team_player_path(natalie_jens.team, natalie_jens), class: 'link-barium' %>
    </div>
    <div class="col-md" align="center">
      <%= link_to "##{paige_banks.number} #{paige_banks.name.split(' ').last}", team_manager.team_player_path(paige_banks.team, paige_banks), class: 'link-barium' %>
